  "bin": {
    "ucat": "./ucat.js"
  },
  "dependencies": {
    "nan": "^2.5.1",
    "node-gyp-build": "^3.0.0",
    "readable-stream": "^2.2.2"
  },
  "devDependencies": {
    "prebuildify": "^2.2.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mafintosh/utp-native.git"
  },
  "author": "Mathias Buus (@mafintosh)",
